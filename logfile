	    media_type VARCHAR(50) NOT NULL,            -- e.g. image, video
	    duration_seconds INT,                       -- null = applies to all durations
	    price_per_day NUMERIC(10,2) NOT NULL,       -- daily base price
	    location_factor NUMERIC(5,2) DEFAULT 1.0,   -- multiplier for premium locations
	
	    created_at TIMESTAMP DEFAULT now(),
	    updated_at TIMESTAMP DEFAULT now()
	);
