	      (
	        SELECT a.id AS entity_id, 'ad' AS entity_type,
	               CASE 
	                 WHEN a.status = 'in_review' THEN 'New ad submitted: ' || a.title
	                 WHEN a.status = 'approved' THEN 'Ad approved: ' || a.title
	                 WHEN a.status = 'rejected' THEN 'Ad rejected: ' || a.title
	                 WHEN a.status = 'paused' THEN 'Ad paused: ' || a.title
	                 WHEN a.status = 'active' THEN 'Ad resumed: ' || a.title
	                 ELSE 'Ad updated: ' || a.title
	               END AS description,
	               a.status_updated_at AS created_at
	        FROM ads a
	        WHERE a.client_id = $1
	      )
	      UNION ALL
