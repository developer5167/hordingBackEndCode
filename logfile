	        WHERE d.client_id = $1
	      )
	      UNION ALL
	      (
	        SELECT p.id, 'payment',
	               'Payment of ₹' || p.amount || ' ' || p.status,
	               p.created_at
	        FROM payments p
	        WHERE p.client_id = $1
	      )
	      ORDER BY created_at DESC
	      LIMIT 20
	    
